<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="sap.wed.invitation.view.Marriage_Venue"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page class="pageBack">
		<customHeader>
			<Bar class="topPadding">
				<contentLeft>
					<Button type="Accept" icon="image/BackPagadi.jpg" text="Back"
						press="back" />
				</contentLeft>
				<contentMiddle>
					<Image src="image/Marriage_Venue.jpg" height="2rem" class="Bismillah" />
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>

			<HBox class="hBox" justifyContent="Center" alignItems="Center">
				<Label text="When :" class="labelClass" />
				<Text text="October 25, 2015, 12:45:00 PM" class="textClass" />
			</HBox>

			<HBox class="hBox1" justifyContent="Center" alignItems="Center">
				<Label text="Where :" class="labelClass" />
				<Text
					text="City Municipal Council Samudaya Bhavan Sector No.19 Behind Dist. Court Navanagar - Bagalkot"
					class="textClass" />
			</HBox>
			<HBox width="100%" justifyContent="Center" alignItems="Center"
				class="coupleClass">
				<Image src="image/Couple1.jpg" width="100%" />
			</HBox>
		</content>
	</Page>
</core:View>
